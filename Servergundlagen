var express = require('express');  //Express importieren

var app = express();  //express wird hier an diesem Programm gebunden

const settings = {    // Port wird hier deklariert
	port: 8000
};

app.listen(settings.port, function() {
	console.log("Der Dienstgeber ist nun auf Port "+settings.port+" verf√ºgbar. ");
});

app.get('/', function (reg, res) {
	res.send('GET Request Hello World');
});
